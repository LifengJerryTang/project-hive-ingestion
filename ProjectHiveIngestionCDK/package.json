{
  "name": "project-hive-ingestion-cdk",
  "version": "0.1.0",
  "bin": {
    "project-hive-ingestion-cdk": "bin/project-hive-ingestion-cdk.js"
  },
  "scripts": {
    "build": "tsc && npm test",
    "clean": "rimraf -rf cdk.out",
    "sam:debug-summarization": "sam local invoke SummarizationLambda7E6C5031 --template-file cdk.out/*SummarizationStack*.template.json --event sam-requests/MessageSummarizationHandler.json --debug-port",
    "sam:debug-gmail": "sam local invoke GmailIngestionlambda48CE6716 --template-file cdk.out/*GmailIngestionStack*.template.json --event sam-requests/GmailIngestionHandler.json --debug-port",
    "watch": "tsc -w",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "deploy": "cdk deploy --all",
    "synth": "cdk synth",
    "diff": "cdk diff",
    "destroy": "cdk destroy --all"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "20.11.30",
    "aws-cdk": "2.135.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.135.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
